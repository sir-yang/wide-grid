Page {
    background: #f9f9f9;
}

.read-list-box {
    padding: 0 30rpx 150rpx;
    border-top: 1px solid #e9e9e9;
    background: #fff;
}

.read-item {
    width: 214rpx;
    margin-top: 30rpx;
}
.read-item:nth-child(3n-1) {
    margin: 30rpx 25rpx 0;
}
.read-item > image {
    display: block;
    width: 100%;
    height: 204rpx;
    margin-bottom: 25rpx;
    -webkit-border-radius: 10rpx;
    -moz-border-radius: 10rpx;
    -o-border-radius: 10rpx;
    border-radius: 10rpx;
}